package aula_II;
import java.lang.*;

public class conta {
	Float saldo;
	int numero;
	
	public conta(int numeroConta, Float saldoConta) {
		this.saldo = saldoConta;
		this.numero = numeroConta;				
	}

	public Float getSaldo() {
		return saldo;
	}

	public void setSaldo(Float saldo) {
		this.saldo = saldo;
	}

	public int getNumero() {
		return numero;
	}

	public void setNumero(int numero) {
		this.numero = numero;
	}
	
	

}
